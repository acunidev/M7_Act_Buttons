<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  package="com.example.tascalayout">

  <application
    android:allowBackup="true"
    android:dataExtractionRules="@xml/data_extraction_rules"
    android:fullBackupContent="@xml/backup_rules"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.TascaLayout"
    tools:targetApi="31">
    <activity
      android:exported="false"
      android:name=".Tascalayout"/>
    <activity
      android:exported="false"
      android:name=".Relativelayout"/>
    <activity
      android:exported="false"
      android:name=".Gridlayout"/>
    <activity
      android:exported="false"
      android:name=".Tablelayout"/>
    <activity
      android:exported="false"
      android:name=".LinearlayoutAct"/>
    <activity
      android:exported="true"
      android:name=".FrameLayout_act">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
  </application>

</manifest>